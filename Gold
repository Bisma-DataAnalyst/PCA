import pandas as pd
import matplotlib.pyplot as plt

# Step 2: Read Excel data using pandas
excel_file_path = "C:\Users\PMLS\Documents\PCA\Gold_Price.xlsx"
df = pd.read_excel(excel_file_path)

# Step 3: Create a histogram
plt.hist(df["Values"], bins=10, color='blue', edgecolor='black')

# Step 4: Add labels and title
plt.xlabel('Values')
plt.ylabel('Frequency')
plt.title('Histogram of Excel Data')

# Step 5: Display the histogram
plt.show()





